{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "acda-mobile Dev Flavor",
      "request": "launch",
      "type": "dart",
      "flutterMode": "debug",
      "program": "lib/main_dev.dart",
      "args": [
        "--flavor",
        "dev",
        // TODO: change API path here
        "--dart-define=apiURL=https://dev-api.example.com",
        "--target",
        "lib/main_dev.dart"
      ]
    },
    {
      "name": "acda-mobile Stg Flavor",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile",
      "program": "lib/main_stg.dart",
      "args": [
        "--flavor",
        "stg",
        "--dart-define=apiURL=https://stg-api.example.com",
        "--target",
        "lib/main_stg.dart"
      ]
    },
    {
      "name": "acda-mobile Prd Flavor",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release",
      "program": "lib/main_prd.dart",
      "args": [
        "--flavor",
        "prd",
        "--dart-define=apiURL=https://prd-api.example.com",
        "--target",
        "lib/main_prd.dart"
      ]
    }
  ]
}
